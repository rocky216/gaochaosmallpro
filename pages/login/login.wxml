<!--pages/login/login.wxml-->
<view class='login'>
  <view class='login_wrap'>
    <view class='login_item'>
      <input type='text' bindinput="usernameHandle" placeholder="手机号" number focus value="{{username}}" ></input>
    </view>
    <view class='login_item' >
      <input type='password' bindinput="passwordHandle"  placeholder="密码" value="{{password}}" ></input>
    </view>
    <view class='login_item' bindtap='goLogin' >
      <button type='primary'>登录</button>
    </view>
  </view>
</view>
