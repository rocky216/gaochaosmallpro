/* pages/track/track.wxss */
.track {
  width: 100%;
  height: 100%;
}
.track_item {
  margin-top: 25rpx;
  box-sizing: border-box;
  background-color: #fff;
  padding: 25rpx;
  border-bottom: 1px solid #ddd;
  border-top: 1px solid #ddd;
}
.track_item button {
  width: 100%;
  height: 100%;
  font-size: 30rpx;
  border-radius: 0;
  border: none;
  padding-top: 50rpx;
}