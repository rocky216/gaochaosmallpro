/* pages/user/user.wxss */
.funcList {
  margin-top: 50rpx;
  display: flex;
  flex-wrap: wrap;
  border-top: 1px solid #ddd;
}
.func_item {
  width: 33.3%;
  text-align: center;
  background-color: #fff;
  padding: 20rpx 0;
  border-right: 1px solid #ddd;
  border-bottom: 1px solid #ddd;
  box-sizing: border-box;
}
.func_item image{
  width: 60rpx;
  height: 60rpx;
}
.head_wrap {
  width: 100%;
  height: 300rpx;
  background-color: #fff;
  border-bottom: 1px solid #ddd;
}
.head {
  width: 50%;
  position: absolute;
  top: 80rpx;
  left: 25%;
  text-align: center;
}
.head image {
  width: 100rpx;
  height: 100rpx;
}